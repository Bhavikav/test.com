                        <script type="text/javascript">
                            function showdiv(id){
                                document.cookie = 'hasJS=true; path=/';
        //alert(id);
        var gettype = id;
                            
        document.getElementById('underlay3').style.display = "block";
        document.getElementById('lightbox3').style.display = "block";
        
        $('body,html').animate({
        scrollTop: 0
        }, 2000);
                            
        if(gettype=='Login')
        { //alert(gettype);
            $("#proceedButton").html('<a class="red_button1_sdn enabled" title="I agree to proceed" href="/users/index/proceed?u=Login">I agree to proceed</a>');
   
        }
        else if (gettype=='Register') 
        { //alert(gettype);
            $("#proceedButton").html('<a class="red_button1_sdn enabled" title="I agree to proceed" href="/users/index/proceed?u=Register">I agree to proceed</a>');
        }
                                                 
        else if (gettype=='HowToUseWebsite') 
        { //alert(gettype);
            $("#proceedButton").html('<a class="red_button1_sdn enabled" title="I agree to proceed" href="/users/index/proceed?u=HowToUseWebsite">I agree to proceed</a>');
  
        }
                                                 
        else if (gettype=='GeneralInfo') 
        { //alert(gettype);
            $("#proceedButton").html('<a class="red_button1_sdn enabled" title="I agree to proceed" href="/users/index/proceed?u=GeneralInfo">I agree to proceed</a>');
   
        }
                                                 
                                             
        //                            
                            
    }
                        </script>
