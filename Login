            <script type="text/javascript">
                $(document).ready(function() {
                    $('#logout').click(function() {
                        $('#underlay_alt_msg').show();
                        $('#logout-confirm-box').show();

                    });            

                });


                function remove_life(base64_id){
                    $('#underlay_alt_msg').show();
                    $('#remove-life-confirm-box').show();
                    $('#delete_link').attr('href','/quotes/LifeDelete/'+base64_id);
                }



                function checkResiUK(resival){ 
                    if (resival == 'n') {
                        $('#underlay_alt_msg_register').show();
                        $('#underlay_alt_msg').show();
                        $('#resident_uk_box_register').show();
                    }
                }


                function remove_life1(base64_id){
                    $('#underlay_alt_msg').show();
                    $('#remove-life-confirm-box1').show();
                    $('#delete_link1').attr('href','/applies/LifeAppDelete/'+base64_id);
                }

                function remove_life2(base64_id){
                    $('#underlay_alt_msg').show();
                    $('#remove-life-confirm-box2').show();
                    $('#delete_link2').attr('href','/quotes/LifeEdit/'+base64_id);
                }

                function submit_life_form(){
                    $('#LifeAdd').submit();
                    $('#LifeEdit').submit();
                    $('#QuoteStart').submit();
                }
                function close_residentUK_box(){
                    $('#underlay_alt_msg').hide();
                    $('#resident_uk_box').hide();

                }
                function close_register_residentUK_box(){
                    $('#underlay_alt_msg_register').hide();
                    $('#underlay_alt_msg').hide();
                    $('#resident_uk_box_register').hide();

                }



            </script>
            <!-- end alert boxes for all site -->


        </div>

